#include <stdio.h>

int main(void){
	int num [9],cont;
	
	  for (cont=0; cont<=9; cont++){
	  printf("Digite um numero inteiro : ");
	  scanf("%d",&num[cont]);
	
}
	printf("\n A inversao dos numeros ficou assim: \n");
	for(cont=9;cont>=0;cont--){
		
	printf("\n %d \n", num[cont]);	
}
	return 0;	
}
	
	
